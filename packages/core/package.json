{
  "name": "@nkmc/core",
  "version": "0.0.2",
  "description": "Server-side SDK for authenticating agent requests via the nkmc gateway",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "import": "./dist/testing.js",
      "require": "./dist/testing.cjs"
    },
    "./scanner": {
      "types": "./dist/scanner.d.ts",
      "import": "./dist/scanner.js",
      "require": "./dist/scanner.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "dependencies": {
    "jose": "^6.1.3",
    "yaml": "^2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chekusu/nkmc-sdk.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ]
}
